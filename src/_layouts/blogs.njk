---
layout: base.njk
---
{% include 'sitenav.njk' %}

<header class="tdbc-hero">
  <div class="tdbc-container">
    <h1 class="tdbc-ink--primary tdbc-mb-none">{{ title }}</h1>
  </div>
</header>
<main class="tdbc-container">
  <ul>
    {%- for blog in collections.blogs -%}
      <li>
        <a href={{ blog.url }}>{{ blog.data.title }}</a>
      </li>
    {%- endfor -%}
  </ul>
</main>